        <Label fx:id="customerLabel" layoutX="40.0" layoutY="80.0" text="Customer ID" />
        <Label fx:id="addressLabel" layoutX="40.0" layoutY="170.0" text="Address" />
        <Label layoutX="40.0" layoutY="215.0" text="Country" />
        <Label fx:id="divisionLabel" layoutX="40.0" layoutY="260.0" text="Division" />
        <Label fx:id="phoneLabel" layoutX="40.0" layoutY="305.0" text="Phone #" />
