        <TextField fx:id="customerTextField" layoutX="115.0" layoutY="80.0" />
        <TextField fx:id="nameTextField" layoutX="115.0" layoutY="125.0" />
        <TextField fx:id="addressTextField" layoutX="115.0" layoutY="170.0" />
        <TextField fx:id="divisionTextField" layoutX="115.0" layoutY="260.0" />
        <TextField fx:id="phoneTextField" layoutX="115.0" layoutY="305.0" />
